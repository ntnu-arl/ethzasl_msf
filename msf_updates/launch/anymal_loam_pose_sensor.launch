<launch>
    <node name="msf_pose_sensor" pkg="msf_updates" type="pose_sensor" clear_params="true" output="screen">

          <remap from="msf_core/imu_state_input" to="/versavis/imu" />
           
          <remap from="msf_updates/transform_input" to="transform_aft_mapped_to_init_CORRECTED" />
          
          <rosparam file="$(find msf_updates)/anymal_pose_sensor_fix.yaml"/>
    </node>

    <node pkg="rosservice" type="rosservice" name="initialize"
       args="call --wait /msf_pose_sensor/pose_sensor/initialize_msf_scale 1"
       launch-prefix="bash -c 'sleep 7; $0 $@'" />
    
</launch>
